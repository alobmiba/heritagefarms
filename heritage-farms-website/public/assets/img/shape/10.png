<svg width="100%" height="128" viewBox="0 0 1200 128" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="organicGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#3A7817;stop-opacity:0.15" />
      <stop offset="25%" style="stop-color:#BFF106;stop-opacity:0.2" />
      <stop offset="50%" style="stop-color:#EAFDE7;stop-opacity:0.25" />
      <stop offset="75%" style="stop-color:#BFF106;stop-opacity:0.2" />
      <stop offset="100%" style="stop-color:#3A7817;stop-opacity:0.15" />
    </linearGradient>
    
    <!-- Farm field pattern -->
    <pattern id="fieldPattern" x="0" y="0" width="40" height="20" patternUnits="userSpaceOnUse">
      <rect width="40" height="20" fill="none"/>
      <line x1="0" y1="10" x2="40" y2="10" stroke="#3A7817" stroke-width="1" opacity="0.1"/>
      <line x1="20" y1="0" x2="20" y2="20" stroke="#3A7817" stroke-width="1" opacity="0.1"/>
    </pattern>
  </defs>
  
  <!-- Main organic curve -->
  <path d="M0,0 
           C200,0 300,20 400,40 
           C500,60 600,80 700,90 
           C800,100 900,110 1000,120 
           C1100,125 1200,128 1200,128 
           L1200,0 Z" 
        fill="url(#organicGradient)"/>
  
  <!-- Subtle field pattern overlay -->
  <path d="M0,0 
           C200,0 300,20 400,40 
           C500,60 600,80 700,90 
           C800,100 900,110 1000,120 
           C1100,125 1200,128 1200,128 
           L1200,0 Z" 
        fill="url(#fieldPattern)" opacity="0.3"/>
  
  <!-- Organic produce silhouettes -->
  <g opacity="0.1">
    <!-- Carrot silhouette -->
    <ellipse cx="200" cy="80" rx="8" ry="16" fill="#FF6B35"/>
    <ellipse cx="200" cy="64" rx="4" ry="12" fill="#228B22"/>
    
    <!-- Tomato silhouette -->
    <circle cx="400" cy="90" r="10" fill="#FF6347"/>
    <ellipse cx="400" cy="78" rx="2" ry="6" fill="#228B22"/>
    
    <!-- Lettuce silhouette -->
    <circle cx="600" cy="100" r="12" fill="#90EE90"/>
    <path d="M588 100 Q600 92 612 100 Q600 108 588 100" fill="#228B22"/>
    
    <!-- Sun silhouette -->
    <circle cx="800" cy="70" r="15" fill="#FFD700"/>
    <g stroke="#FFD700" stroke-width="3">
      <line x1="800" y1="50" x2="800" y2="45"/>
      <line x1="800" y1="95" x2="800" y2="90"/>
      <line x1="785" y1="70" x2="780" y2="70"/>
      <line x1="815" y1="70" x2="810" y2="70"/>
    </g>
  </g>
</svg>
