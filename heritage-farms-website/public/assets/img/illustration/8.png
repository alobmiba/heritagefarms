<svg width="120" height="120" viewBox="0 0 120 120" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="120" height="120" fill="#EAFDE7" opacity="0.3"/>
  
  <!-- Farm field pattern -->
  <defs>
    <pattern id="fieldPattern" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
      <rect width="20" height="20" fill="#BFF106" opacity="0.1"/>
      <line x1="0" y1="10" x2="20" y2="10" stroke="#3A7817" stroke-width="1" opacity="0.3"/>
    </pattern>
  </defs>
  
  <!-- Main farm field -->
  <rect x="10" y="40" width="100" height="60" fill="url(#fieldPattern)" rx="8"/>
  
  <!-- Organic produce icons -->
  <!-- Carrot -->
  <g transform="translate(25, 50)">
    <ellipse cx="8" cy="12" rx="4" ry="8" fill="#FF6B35"/>
    <ellipse cx="8" cy="4" rx="2" ry="6" fill="#228B22"/>
  </g>
  
  <!-- Tomato -->
  <g transform="translate(45, 50)">
    <circle cx="8" cy="8" r="6" fill="#FF6347"/>
    <ellipse cx="8" cy="2" rx="1" ry="3" fill="#228B22"/>
  </g>
  
  <!-- Lettuce -->
  <g transform="translate(65, 50)">
    <circle cx="8" cy="8" r="6" fill="#90EE90"/>
    <path d="M4 8 Q8 4 12 8 Q8 12 4 8" fill="#228B22"/>
  </g>
  
  <!-- Sun -->
  <g transform="translate(85, 20)">
    <circle cx="12" cy="12" r="8" fill="#FFD700"/>
    <g stroke="#FFD700" stroke-width="2">
      <line x1="12" y1="2" x2="12" y2="0"/>
      <line x1="12" y1="24" x2="12" y2="22"/>
      <line x1="2" y1="12" x2="0" y2="12"/>
      <line x1="24" y1="12" x2="22" y2="12"/>
      <line x1="4" y1="4" x2="2" y2="2"/>
      <line x1="22" y1="22" x2="20" y2="20"/>
      <line x1="4" y1="20" x2="2" y2="22"/>
      <line x1="22" y1="2" x2="20" y2="4"/>
    </g>
  </g>
  
  <!-- Water drop for irrigation -->
  <g transform="translate(15, 25)">
    <path d="M6 0 Q6 4 3 8 Q6 6 9 8 Q6 4 6 0" fill="#87CEEB"/>
  </g>
  
  <!-- Heritage Farms text -->
  <text x="60" y="110" text-anchor="middle" fill="#3A7817" font-size="8" font-weight="bold" font-family="Arial, sans-serif">HERITAGE FARMS</text>
</svg>
